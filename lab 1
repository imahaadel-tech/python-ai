cat_a = [1, 2, 3, 4, 5]
cat_b = [10, 20, 30]

def calculate_stats(data):
    return {
        "mean": sum(data) / len(data),
        "max": max(data)
    }

stats_a = calculate_stats(cat_a)
stats_b = calculate_stats(cat_b)

print("Category A Stats:")
print(f"Mean: {stats_a['mean']}")
print(f"Max: {stats_a['max']}")

print("Category B Stats:")
print(f"Mean: {stats_b['mean']}")
print(f"Max: {stats_b['max']}")    
